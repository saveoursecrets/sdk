{
  "name": "sos-web-gui",
  "version": "1.0.0",
  "description": "",
  "author": "muji <muji@tmpfs.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist ./.parcel-cache",
    "types": "tsc --noEmit --project tsconfig.json",
    "assets": "mkdir dist && cp -r ./assets ./dist",
    "public": "rm -rf ../workspace/server/public && cp -r dist ../workspace/server/public",
    "build": "parcel build index.html && yarn assets && yarn public",
    "start": "yarn assets && parcel index.html --port 8080",
    "prettier": "prettier --write . --no-color",
    "lint": "eslint src",
    "test:headed": "FORCE_COLOR=0 playwright test --headed",
    "test": "FORCE_COLOR=0 playwright test"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/uuid": "^8.3.4",
    "comlink": "^4.3.1",
    "history": "^5.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0",
    "sos-wasm": "file:../workspace/wasm/pkg",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.3",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.6.3"
  }
}
