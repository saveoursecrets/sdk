{
  "compilerOptions": {
    "lib": ["dom"],
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es2022",
    "target": "es2017",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}
